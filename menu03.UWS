DIM WINDOWNAME[] = "BlueStacks_kzk"

DIM MENU[] = "NPC攻撃","ハンマー自動集結"

IF LENGTH(PARAM_STR[0]) > 0 THEN
	MSGBOX("NO: T" + PARAM_STR[0])
ELSE

SLCT = SLCTBOX(SLCT_BTN,0,0,0,"",MENU)

SELECT SLCT
  CASE -1
    EXIT
  CASE  1
    FOR I = 0 TO LENGTH(WINDOWNAME) - 1
      SLEEP(1)
      CALL attack_npc.UWS(WINDOWNAME[I])
    NEXT
  CASE  2
    FOR I = 0 TO LENGTH(WINDOWNAME) - 1
      SLEEP(1)
      CALL attack_auto.UWS(WINDOWNAME[I])
    NEXT
SELEND


ENDIF
