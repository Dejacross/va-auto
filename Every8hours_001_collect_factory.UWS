VIVI_WINDOW = GETID( PARAM_STR[0] )

WINDOW01_X = STATUS( VIVI_WINDOW, ST_X)
WINDOW01_Y = STATUS( VIVI_WINDOW, ST_Y)

// 強化工場

// クリック座標
DIM CRICK_POS_X[] = 80,310,130,220,60
DIM CRICK_POS_Y[] = 360 ,760,760,760,130


// ループ開始

ACW(GETID(VIVI_WINDOW))

FOR I = 0 TO LENGTH(CRICK_POS_X) -1

BTN(LEFT,CLICK, WINDOW01_X + CRICK_POS_X[I] + RANDOM(3), WINDOW01_Y + CRICK_POS_Y[I] + RANDOM(3), 800 + (RANDOM(3) * 100))

NEXT



